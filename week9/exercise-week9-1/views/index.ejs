    <!-- 用户信息部分 -->
    <div id="userhome">
        <header>
            <div class="user-toolbar">
                <div class="user-toolbar-right">
                    <div class="toolbar-back-btn btn-baidu"></div>
                </div>
            </div>
            <div class="user-content">
                <div class="user-info">
                    <div class="photo">
                        <div class="imgwrapper">
                            <img src="/images/person.png" height="150" width="150">
                        </div>
                    </div>
                    <div class="login">
                        <span>立即登录</span>
                    </div>
                </div>
                <ul class="user-content-main">
                    <li class="msg">
                        <i></i> 我的消息
                    </li>
                    <li class="favor">
                        <i></i> 我的收藏
                    </li>
                    <li class="model">
                        <i></i> 无图模式
                    </li>
                    <li class="slide">
                        <i></i> 左右滑动
                    </li>
                </ul>
            </div>
        </header>
    </div>
    <!-- 用户搜索部分 -->
    <div id="usersearch">
        <header>
            <div class="usersearch-toolbar">
                <div class="usersearch-toolbar-left">
                    <div class="toolbar-back2-btn btn-baidu"></div>
                    <span>搜新闻</span>
                </div>
            </div>
        </header>
        <div class="usersearch-content">
            <div class="usersearch-content-searchbar">
                <form action="#">
                    <input type="text" name="search" id="search" />
                    <input type="submit" value="百度一下" name="submit" id="submit" />
                </form>
            </div>
            <ul class="hotsearch">
                <li>
                    <h2>热搜榜</h2>
                </li>
                <li>
                    <div>习近平考察小岗村</div>
                </li>
